# StudX Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Public Supabase URL - safe to expose
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here

# Public Supabase Key (anon) - safe to expose  
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Secret Supabase Key (service_role) - MUST BE KEPT SECRET
SUPABASE_SECRET_KEY=your_secret_key_here

# Razorpay Payment Gateway
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_here

# ImgBB API for image uploads
IMGBB_API_KEY=your_imgbb_key_here

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
